head -n 1 genome.nfa > concat.nfa
grep -v '>' genome.nfa >> concat.nfa
~/dev/glimmer3.02/bin/long-orfs -n -t 1.15 --max_olap 30 --linear --min_len 50 concat.nfa concat-alt.coord
~/dev/glimmer3.02/bin/extract -t concat.nfa concat.coord > concat.extract
~/dev/glimmer3.02/bin/build-icm orf.00.model < concat.extract
~/dev/glimmer3.02/bin/glimmer3 --linear concat.nfa orf.00.model concat
extractSeq.pl -f concat.nfa -F concat.predict -o concat_orf30.nfa -p 30 -v -S 2 -E 3
elph concat_orf30.nfa -b -a -g LEN=5 | & tee elph.stdout
